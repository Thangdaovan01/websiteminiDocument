<body>
    <div class="header-container">
        <div class="logo">
            <a class="" href="/">EXCEL</a>
        </div>
        <div class="nav-container">
            <a class="" id="priceList" href="/document">Tài liệu</a>
            <a class="" id="priceList" href="/documentFilename">PDF</a>
        </div>
        <div class="create-row-container">
            <button id="create" title="Thêm dữ liệu mới" class="create-btn"><i class="fa-solid fa-file-circle-plus"></i></button>
        </div>
        <div class="search-container">
            <form class="search-form" id="search_form" >
                <input type="text" name="searchText" class="searchText">
                <button type="submit"><i class="fa-solid fa-magnifying-glass"></i></button>
            </form>
        </div>
        <div class="logout-container">
            <button id="logout" title="Đăng xuất" class="logout-btn"><i class="fa-solid fa-right-from-bracket"></i></button>
        </div>

    </div>
    <main>
        <h1>File Detail</h1>
        <!-- <canvas id="pdfCanvas" width="800" height="600"></canvas> -->
        <div class="file-container">
            <iframe id="file-iframe" src ="" style="height:1000px; width:800px;"></iframe>
        </div>

    </main>
</body>
<script src="/js/documentfile.js"></script>
